
#include <stdio.h>
#include <math.h>

int main() {

  float altura, largura, area;
  
  
  printf("Digite o valor do altura: ");
  scanf("%f",&altura);
  printf("Digite o valor da largura: ");
  scanf("%f",&largura);

  area = altura*largura;
  printf("O terreno possui %.1f metros quadrados de Ã¡rea",area);


    

    return 0;
}
